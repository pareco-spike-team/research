{
  "name": "galnetpedia",
  "version": "1.0.0",
  "description": "Galnet",
  "main": "index.js",
  "repository": "git@github.com:pareco-spike-team/research.git",
  "author": "Cmdr Clannad <clannad@spam.org>",
  "license": "MIT",
  "scripts": {
    "test": "mocha --exit tests/**/*Tests.js",
    "install_packages": "yarn install",
    "precopy": "yarn install_packages && if [ -d './dist' ]; then rm -Rf ./dist; fi && mkdir -p dist/fontawesome",
    "copy": "cp -a ./node_modules/@fortawesome/fontawesome-free/. ./dist/fontawesome/",
    "prebuild": "yarn test && yarn copy && cp ./frontend/src/index.html ./dist/index.html && cp ./frontend/src/galnetpedia.css ./dist/galnetpedia.css",
    "build": "./node_modules/.bin/elm make ./frontend/src/Main.elm --output=./dist/galnetpedia.js --debug",
    "build_release": "yarn prebuild && ./node_modules/.bin/elm make ./frontend/src/Main.elm --output=./dist/galnetpedia.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "~5.14.0",
    "an-array-of-english-words": "~1.3.1",
    "axios": "~0.20.0",
    "body-parser": "~1.19.0",
    "case": "~1.6.3",
    "compression": "~1.7.4",
    "express": "~4.17.1",
    "express-session": "~1.17.1",
    "helmet": "~3.16.0",
    "moment": "~2.27.0",
    "morgan": "~1.9.1",
    "neo4j-driver": "~1.7.7",
    "p3p": "~0.0.2",
    "qs": "~6.9.4",
    "shortid": "~2.2.14"
  },
  "devDependencies": {
    "chai": "~4.2.0",
    "elm": "~0.19.1-3",
    "elm-format": "~0.8.3",
    "jshint": "~2.12.0",
    "mocha": "~8.1.3",
    "serverless": "~1.80.0",
    "serverless-webpack": "~5.3.4",
    "webpack": "~4.44.1"
  }
}
